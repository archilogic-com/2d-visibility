{
    "compilerOptions": {
        "sourceMap": true,
        "strict": true,
        "noImplicitReturns": true,
        "module": "ESNext",
        "moduleResolution": "node",
        "target": "ESNext",
        "noEmitHelpers": true,
        "types": [
            "vitest/globals"
        ],
        "skipLibCheck": true,
    },
    "include": [
        "./src/**/*.ts"
    ]
}